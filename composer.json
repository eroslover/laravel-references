{
    "name": "eroslover/laravel-references",
    "description": "References between models in Laravel",
    "keywords": [
        "laravel-references",
        "references",
        "laravel",
        "models",
        "eroslover"
    ],
    "homepage": "https://github.com/eroslover/laravel-references",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.1",
        "illuminate/database": "~5.1",
        "illuminate/support": "~5.1",
        "illuminate/contracts": "~5.1",
        "illuminate/http": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "authors": [
        {
            "name": "Yaroslav Shikovets",
            "email": "yaroslav.shikovets@yandex.ru",
            "role": "Developer"
        },
        {
            "name": "Ivin Sergey",
            "email": "phirames@yandex.ru",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eroslover\\References\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eroslover\\References\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eroslover\\References\\ReferencesServiceProvider"
            ]
        }
    }
}
