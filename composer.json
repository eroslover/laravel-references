{
    "name": "eroslover/laravel-references",
    "description": "Model references for laravel",
    "keywords": [
        "laravel-references",
        "references",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "authors": [
        {
            "name": "Yaroslav Shikovets",
            "email": "yaroslav.shikovets@yandex.ru",
            "role": "Developer"
        },
        {
            "name": "Ivin Sergey",
            "email": "phirames@yandex.ru",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eroslover\\References\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eroslover\\References\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eroslover\\References\\ReferencesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
